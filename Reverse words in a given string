


class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String S)
    {
        // code here 
        String k[] = S.split("\\.");
		StringBuffer bf = new StringBuffer();
		for(int i = k.length-1;i>=0;i--)
			bf.append(k[i]).append(".");
		
		return bf.substring(0,bf.length()-1);
    }
}
