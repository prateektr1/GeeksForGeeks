class Solution
{    
    //Function is to check whether two strings are anagram of each other or not.
    public static boolean isAnagram(String a,String b)
    {
        
        // Your code here
        
        char a1[] = a.toCharArray();
        char b1[] = b.toCharArray();
        
        Arrays.sort(a1);
        Arrays.sort(b1);
        
        return Arrays.equals(a1,b1);
        
    }
}
